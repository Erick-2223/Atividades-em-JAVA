//ATIVIDADE 01 ESTRUTURA DE REPETIÇÃO
// SOMAR OS NUMEROS DE 1 A 100
package trabalho;
import java.util.Scanner;

public class Trabalho {

    public static void main(String[] args) {
        int soma = 0;

        for (int i = 1; i <= 100; i++) {
            soma += i;
        }

        System.out.println("A soma dos números de 1 a 100 é: " + soma);
    }
}
